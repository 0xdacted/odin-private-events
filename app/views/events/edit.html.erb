<%= form_with model: @event do |form| %>
  Edit title: <%= form.text_field :title, value: @event.title %>
  Edit details: <%= form.text_field :body, value: @event.body %>
  Edit location: <%= form.text_field :location, value: @event.location %>
  Edit date: <%= form.datetime_select :date, value: @event.date %>
<%= form.submit %>
<% end %>