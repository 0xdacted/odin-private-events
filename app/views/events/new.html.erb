<%= form_with model: @event do |form| %>
  <%= form.label "Title:"%> 
  <%= form.text_field :title, placeholder: "insert title here" %>
  <%= form.label "Body:"%>  
  <%= form.text_field :body, placeholder: "insert body content here" %>
  <%= form.label "Location:"%> 
  <%= form.text_field :location, placeholder: "insert location here" %>
  <%= form.label "Date:"%>  
  <%= form.datetime_select :date %>
  <%= form.submit %>
<% end %>